{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "preBuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:nodemon:dev": "nodemon --config nodemon.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "ttt": "tsc: watch - tsconfig.build.json",
    "test:watch": "jest --watch",
    "test:watch-all": "jest --watchAll --verbose",
    "test:watch-all-detect": "jest --watchAll --verbose --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./src/config/database/orm-migrations.config.ts",
    "migration-create": "npm run typeorm:cli -- migration:create -n created",
    "migration-generate": "npm run typeorm:cli -- migration:generate -n generated",
    "migrations-run": "npm run typeorm:cli -- migration:run",
    "migrations-revert": "npm run typeorm:cli -- migration:revert"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.2.4",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.2.4",
    "@types/ua-parser-js": "^0.7.36",
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "get-image-colors": "^2.0.1",
    "googleapis": "^92.0.0",
    "jimp": "^0.16.1",
    "joi": "^17.5.0",
    "moment": "^2.29.1",
    "nest-winston": "^1.6.2",
    "nestjs-i18n": "^8.2.2",
    "nestjs-typeorm-paginate": "^3.1.3",
    "pg": "^8.7.1",
    "redis": "^4.0.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "response-time": "^2.3.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.4.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
